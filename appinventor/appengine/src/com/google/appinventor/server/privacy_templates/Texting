@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ai: <http://dig.csail.mit.edu/2014/PrivacyInformer/appinventor#> .
@prefix android: <http://dig.csail.mit.edu/2014/PrivacyInformer/android#> .
@prefix texting: <http://dig.csail.mit.edu/2014/PrivacyInformer/Texting#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# texting is component

# Component Description
texting:TextingComponent  
    a   owl:Class;
    rdfs:label  "Texting component";
    rdfs:subClassOf ai:Component;
    ai:description  "sends text messages to specific phone numbers";
    ai:category     ai:Social;

# Privacy Implication and Common Usage
    ai:dataUsage    ai:SocialNetworking;
    ai:examples     "Skype";
    ai:purpose      ai:DataAccess.

