<!--FileUploadWizard.ui.xml -->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:w="urn:import:com.google.appinventor.client.widgets"
             xmlns:wiz="urn:import:com.google.appinventor.client.wizards"
             ui:generatedFormat="com.google.gwt.i18n.server.PropertyCatalogFactory"
             ui:generatedKeys="com.google.gwt.i18n.server.keygen.MethodNameKeyGenerator"
             ui:generateLocales="default">
  <ui:with field="messages" type="com.google.appinventor.client.OdeMessages" />
  <wiz:Dialog ui:field="uploadDialog" caption="{messages.fileUploadWizardCaption}" styleName="ode-DialogBox">
    <g:FlowPanel>
      <g:FileUpload ui:field="upload" name="uploadFile" />
      <g:FlowPanel styleName="buttonRow" >
        <g:Button ui:field='cancelButton' text='{messages.cancelButton}' stylePrimaryName="ode-ProjectListButton"  />
        <g:Button ui:field='okButton' text='{messages.okButton}' stylePrimaryName="ode-ProjectListButton" />
      </g:FlowPanel>
    </g:FlowPanel>
  </wiz:Dialog>
</ui:UiBinder>
