<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.3.0//EN" "http://www.gwtproject.org/doctype/2.8.1/gwt-module.dtd">
<module rename-to="webemu">
  <entry-point class="edu.mit.appinventor.webemu.WebEmulator" />

  <inherits name="com.google.appinventor.common.CommonUtils" />
  <inherits name="com.google.appinventor.common.CommonVersion" />
  <inherits name="com.google.appinventor.components.CommonConstants" />
  <inherits name="androidemu" />
  <inherits name="com.google.appinventor.components.AiComponents" />

  <inherits name='com.google.gwt.user.UserAgent'/>
  <set-property name="user.agent" value="safari,gecko1_8" />
  <inherits name="com.google.gwt.user.User" />
  <inherits name="com.google.gwt.i18n.I18N"/>
  <inherits name="com.google.gwt.json.JSON" />
  <inherits name="com.google.gwt.libideas.ImmutableResources" />
  <inherits name="com.google.gwt.logging.Logging"/>
  <inherits name="com.google.gwt.resources.Resources" />
  <inherits name="com.google.gwt.widgetideas.WidgetIdeas" />
  <inherits name="com.google.gwt.widgetideas.GWTCanvas" />
  <inherits name="com.google.common.base.Base" />
  <inherits name="com.google.common.collect.Collect" />
  <inherits name="com.google.appinventor.components.CommonConstants" />
  <inherits name="com.google.appinventor.common.CommonUtils" />
  <inherits name="com.google.appinventor.common.CommonVersion" />
  <inherits name="com.google.common.annotations.Annotations" />
  <inherits name="com.google.gwt.xml.XML" />

  <set-property name="gwt.logging.logLevel" value="INFO"/>
  <set-property name="gwt.logging.enabled" value="TRUE"/>
  <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>
  <set-configuration-property name="CssResource.enableGss" value="true" />

  <super-source path="com/google/gwt/emul" />
  <!--<source path="" />-->
  <source path="android" />
  <!--
  <source path="android/view" />
  <source path="android/widget" />
  <source path="android/graphics/drawable" />
  -->
  <source path="androidx" />
  <!--
  <source path="androidx/appcompat/app" />
  <source path="androidx/appcompat/view" />
  -->
  <source path="com/google/appinventor/components" />
  <source path="com/google/appinventor/components/common" />
  <source path="com/google/appinventor/components/runtime/client" />
  <source path="com/google/protobuf" />
  <!--
  <source path="com/google/appinventor/components/runtime/util" />
  -->
  <source path="edu" />
  <!--
  <source path="edu/mit/appinventor/webemu" />
  -->

  <!--
  <extend-property name="locale" values="en" />
  <set-property-fallback name="locale" value="en" />
  -->
  <collapse-all-properties />
</module>
